(set-option :print-success false)
(set-option :produce-models true)
(set-info :souce |A tight rhombus that only contains a few solutions.  This benchmark is designed to be hard for cut engines.
Authors: The SMTInterpol team|)
(set-info :status sat)
(set-info :category "crafted")
(set-logic QF_LIRA)
(declare-fun x () Int)
(declare-fun y () Real)
(declare-fun z () Int)
(assert (and
	(<= 0 (- (to_real (* 28300000 x)) (* 24500001.0 y)))
	(<= (- (to_real (* 28300000 x)) (* 24500001.0 y)) 99999.0)
	(<= 1 (- (to_real (* 28300001 x)) (* 24500000.0 y)))
	(<= (- (to_real (* 28300001 x)) (* 24500000.0 y)) 100000.0)))
(assert (<= 0 (- y (to_real z))))
(assert (<= (- y (to_real z)) (/ 68 24500001)))
(check-sat)
(get-model)
(exit)
